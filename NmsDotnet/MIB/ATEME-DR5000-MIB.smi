   MAX-ACCESS not-accessible
   STATUS current
   DESCRIPTION "."
   ::= { dr5000PresetPreset 1 }

dr5000PresetPresetEntry OBJECT-TYPE
   SYNTAX Dr5000PresetPresetEntry
   MAX-ACCESS not-accessible
   STATUS current
   DESCRIPTION "."
   INDEX {
      dr5000PresetPresetPresetIdx
   }
   ::= { dr5000PresetPresetTable 1 }

Dr5000PresetPresetEntry ::=
   SEQUENCE {
      dr5000PresetPresetPresetIdx RowIndex,
      dr5000PresetPresetName DisplayString
   }

dr5000PresetPresetPresetIdx OBJECT-TYPE
   SYNTAX RowIndex
   MAX-ACCESS not-accessible
   STATUS current
   DESCRIPTION "."
   ::= { dr5000PresetPresetEntry 1 }

dr5000PresetPresetName OBJECT-TYPE
   SYNTAX DisplayString
   MAX-ACCESS read-only
   STATUS current
   DESCRIPTION "Name of the preset (user defined)"
   ::= { dr5000PresetPresetEntry 2 }

dr5000Licenses OBJECT IDENTIFIER ::= { dr5000 13 }
dr5000LicensesList OBJECT IDENTIFIER ::= { dr5000Licenses 1 }

dr5000LicensesListTable OBJECT-TYPE
   SYNTAX SEQUENCE OF Dr5000LicensesListEntry
   MAX-ACCESS not-accessible
   STATUS current
   DESCRIPTION "."
   ::= { dr5000LicensesList 1 }

dr5000LicensesListEntry OBJECT-TYPE
   SYNTAX Dr5000LicensesListEntry
   MAX-ACCESS not-accessible
   STATUS current
   DESCRIPTION "."
   INDEX {
      dr5000LicensesListListIdx
   }
   ::= { dr5000LicensesListTable 1 }

Dr5000LicensesListEntry ::=
   SEQUENCE {
      dr5000LicensesListListIdx RowIndex,
      dr5000LicensesListDescription DisplayString,
      dr5000LicensesListIsValueBoolean TruthValue,
      dr5000LicensesListIsValue TruthValue,
      dr5000LicensesListNValue UnsignedValue
   }

dr5000LicensesListListIdx OBJECT-TYPE
   SYNTAX RowIndex
   MAX-ACCESS not-accessible
   STATUS current
   DESCRIPTION "."
   ::= { dr5000LicensesListEntry 1 }

dr5000LicensesListDescription OBJECT-TYPE
   SYNTAX DisplayString
   MAX-ACCESS read-only
   STATUS current
   DESCRIPTION "Description of the license"
   ::= { dr5000LicensesListEntry 2 }

dr5000LicensesListIsValueBoolean OBJECT-TYPE
   SYNTAX TruthValue
   MAX-ACCESS read-only
   STATUS current
   DESCRIPTION "If true, the value of this license is in the node 'isValue', if false, the value is in the node 'nValue'"
   ::= { dr5000LicensesListEntry 3 }

dr5000LicensesListIsValue OBJECT-TYPE
   SYNTAX TruthValue
   MAX-ACCESS read-only
   STATUS current
   DESCRIPTION "When 'isValueBoolean' is true, determines the value of the license"
   ::= { dr5000LicensesListEntry 4 }

dr5000LicensesListNValue OBJECT-TYPE
   SYNTAX UnsignedValue
   MAX-ACCESS read-only
   STATUS current
   DESCRIPTION "When 'isValueBoolean' is false, determines the value of the license"
   ::= { dr5000LicensesListEntry 5 }


END


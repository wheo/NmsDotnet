dr5000ChannelConfigurationInputType OBJECT-TYPE
   SYNTAX INTEGER {
      ip(1),
      asi(2),
      sat(3),
      ds3(4),
      zixi(5),
      ultraIp(6),
      srt(7)
   }
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "Determines the decoder input"
   ::= { dr5000ChannelConfigurationInput 1 }

dr5000ChannelConfigurationInputIp OBJECT IDENTIFIER ::= { dr5000ChannelConfigurationInput 2 }
dr5000ChannelConfigurationInputIpAddress OBJECT-TYPE
   SYNTAX InetAddress
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "Address of the multicast group to join. '0.0.0.0' means unicast. When multicast is chosen, the decoder will broadcast an IGMP join message to register to the specified multicast address. When unicast is chosen, the decoder will listen to incoming stream on the selected input interface"
   ::= { dr5000ChannelConfigurationInputIp 1 }

dr5000ChannelConfigurationInputIpPort OBJECT-TYPE
   SYNTAX InetPortNumber
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "Input port on which the decoder will listen to incoming stream"
   ::= { dr5000ChannelConfigurationInputIp 2 }

dr5000ChannelConfigurationInputIpInterface OBJECT-TYPE
   SYNTAX INTEGER {
      stream1(1),
      stream2(2)
   }
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "Selects the ethernet input interface"
   ::= { dr5000ChannelConfigurationInputIp 3 }

dr5000ChannelConfigurationInputIpIsFecEnabled OBJECT-TYPE
   SYNTAX TruthValue
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "When enabled, the decoder will try to lock on one several FEC streams, and will automatically performs correction if required"
   ::= { dr5000ChannelConfigurationInputIp 4 }

dr5000ChannelConfigurationInputIpBufferDuration OBJECT-TYPE
   SYNTAX UnsignedValue
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "Determines the buffer duration of the IP input. This buffer is responsible for IP dejittering and for FEC reconstruction. Large values implies more latency, but may enhance output signal stability and FEC reconstruction"
   ::= { dr5000ChannelConfigurationInputIp 5 }

dr5000ChannelConfigurationInputIpSsm OBJECT IDENTIFIER ::= { dr5000ChannelConfigurationInputIp 6 }
dr5000ChannelConfigurationInputIpSsmIsEnabled OBJECT-TYPE
   SYNTAX TruthValue
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "Enables Source Specific Multicast"
   ::= { dr5000ChannelConfigurationInputIpSsm 1 }

dr5000ChannelConfigurationInputIpSsmMode OBJECT-TYPE
   SYNTAX INTEGER {
      include(1),
      exclude(2)
   }
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "Filter mode"
   ::= { dr5000ChannelConfigurationInputIpSsm 2 }

-- List of addresses to include or exclude, depending on the specified filtering mode
dr5000ChannelConfigurationInputIpSsmAddress OBJECT IDENTIFIER ::= { dr5000ChannelConfigurationInputIpSsm 3 }

dr5000ChannelConfigurationInputIpSsmAddressTable OBJECT-TYPE
   SYNTAX SEQUENCE OF Dr5000ChannelConfigurationInputIpSsmAddressEntry
   MAX-ACCESS not-accessible
   STATUS current
   DESCRIPTION "List of addresses to include or exclude, depending on the specified filtering mode"
   ::= { dr5000ChannelConfigurationInputIpSsmAddress 1 }

dr5000ChannelConfigurationInputIpSsmAddressEntry OBJECT-TYPE
   SYNTAX Dr5000ChannelConfigurationInputIpSsmAddressEntry
   MAX-ACCESS not-accessible
   STATUS current
   DESCRIPTION "List of addresses to include or exclude, depending on the specified filtering mode"
   INDEX {
      dr5000ChannelConfigurationInputIpSsmAddressIdx
   }
   ::= { dr5000ChannelConfigurationInputIpSsmAddressTable 1 }

Dr5000ChannelConfigurationInputIpSsmAddressEntry ::=
   SEQUENCE {
      dr5000ChannelConfigurationInputIpSsmAddressIdx RowIndex,
      dr5000ChannelConfigurationInputIpSsmAddressAddress InetAddress
   }

dr5000ChannelConfigurationInputIpSsmAddressIdx OBJECT-TYPE
   SYNTAX RowIndex
   MAX-ACCESS not-accessible
   STATUS current
   DESCRIPTION "."
   ::= { dr5000ChannelConfigurationInputIpSsmAddressEntry 1 }

dr5000ChannelConfigurationInputIpSsmAddressAddress OBJECT-TYPE
   SYNTAX InetAddress
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "List of addresses to include or exclude, depending on the specified filtering mode"
   ::= { dr5000ChannelConfigurationInputIpSsmAddressEntry 2 }

dr5000ChannelConfigurationInputIpFailover OBJECT IDENTIFIER ::= { dr5000ChannelConfigurationInputIp 7 }

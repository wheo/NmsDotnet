dr5000ChannelCommandLoad OBJECT IDENTIFIER ::= { dr5000ChannelCommand 2 }
dr5000ChannelCommandLoadPreset OBJECT-TYPE
   SYNTAX UnsignedValue
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "Index of the preset which will be used to get the configuration on a 'load' action"
   ::= { dr5000ChannelCommandLoad 1 }

dr5000ChannelCommandSave OBJECT IDENTIFIER ::= { dr5000ChannelCommand 3 }
dr5000ChannelCommandSavePreset OBJECT-TYPE
   SYNTAX UnsignedValue
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION "Index of the preset which will be used to save the current configuration on a 'save' action"
   ::= { dr5000ChannelCommandSave 1 }

dr5000ChannelConfiguration OBJECT IDENTIFIER ::= { dr5000Channel 2 }
dr5000ChannelConfigurationName OBJECT-TYPE
   SYNTAX DisplayString
   MAX-ACCESS read-write
   STATUS current
